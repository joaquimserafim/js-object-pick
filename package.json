{
  "name": "js-object-pick",
  "version": "1.0.0",
  "description": "Return a copy of the object only containing the specified properties",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover tape test.js",
    "jshint": "jshint -c .jshintrc *.js",
    "code-style": "jscs -p google *.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/js-object-pick.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "keywords": [
    "pick",
    "object",
    "filter",
    "keys",
    "js",
    "util",
    "utilities",
    "properties",
    "property",
    "key"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/js-object-pick/issues"
  },
  "homepage": "https://github.com/joaquimserafim/js-object-pick",
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "pre-commit": "^1.0.2",
    "tape": "^3.5.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
}
